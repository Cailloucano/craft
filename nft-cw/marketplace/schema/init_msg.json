{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "denom",
    "fee_receive_address",
    "name",
    "platform_fee"
  ],
  "properties": {
    "denom": {
      "type": "string"
    },
    "fee_receive_address": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "platform_fee": {
      "type": "integer",
      "format": "uint128",
      "minimum": 0.0
    }
  }
}
